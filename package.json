{
  "name": "appcloudwebapp",
  "version": "1.0.0",
  "description": "AppCloud Web App built with the MERN stack",
  "main": "server.js",
  "scripts": {
    "clietn-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon --ignore client/ server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
